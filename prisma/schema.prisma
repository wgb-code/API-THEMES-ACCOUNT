generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Enterprise {
  id                Int @id @default(autoincrement())
  name              String
  email             String
  password          String
  address           String
  generalbg         String?
  generalcolor      String?
  highlightsbg      String?
  highlightscolor   String?
}